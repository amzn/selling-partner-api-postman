		{
			"name": "listings/2021-08-01/items/{seller Id}/{sku}",
			"item": [
				{
					"name": "patch Listings Item",
					"id": "481562e0-85c4-447d-b237-58648b6899a4",
					"protocolProfileBehavior": {
						"disableUrlEncoding": false,
						"disableBodyPruning": true
					},
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "PATCH",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "x-amz-access-token",
								"value": "{{access_token}}",
								"type": "text"
							},
							{
								"key": "",
								"value": "",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"productType\": \"PRODUCT\",\n    \"patches\": [\n        {\n            \"op\": \"replace\",\n            \"path\": \"/attributes/purchasable_offer\",\n            \"value\": [\n                {\n                    \"marketplace_id\": \"{{marketplace}}\",\n                    \"currency\": \"USD\",\n                    \"our_price\": [\n                        {\n                            \"schedule\": [\n                                {\n                                    \"value_with_tax\": 10\n                                }\n                            ]\n                        }\n                    ]\n                }\n            ]\n        }\n    ]    \n}"
						},
						"url": {
							"raw": "{{baseUrl}}/listings/2021-08-01/items/:sellerId/:sku?marketplaceIds={{marketplace}}",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"listings",
								"2021-08-01",
								"items",
								":sellerId",
								":sku"
							],
							"query": [
