									"key": "marketplaceIds",
									"value": "{{marketplace}}"
								},
								{
									"key": "issueLocale",
									"value": "en_US",
									"disabled": true
								},
								{
									"key": "includedData",
									"value": "summaries,issues,offers,fulfillmentAvailability,attributes",
									"disabled": true
								},
								{
									"key": "mode",
									"value": "VALIDATION_PREVIEW",
									"disabled": true
								}
							],
							"variable": [
